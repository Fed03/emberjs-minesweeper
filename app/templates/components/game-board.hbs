<div class="board-header">
  <span class="board-counter" data-test-remaining-mines-counter>{{remainingMines}}</span>
  <span class="board-counter" data-test-elapsed-time>{{model.elapsedTime}}</span>
</div>
<div>
  {{#each cellsByRow as |cellRow|}}
    <div class="row">
      {{#each cellRow as |cellModel|}}
        {{board-cell data-test-cell=cellModel.position model=cellModel onOpenCell=(action "openedCell") onFlagCell=(action "flaggedCell")}}
      {{/each}}
    </div>
  {{/each}}
</div>